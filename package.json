{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "jslint-lite",
    "description": "api documentation for [jslint-lite (v2017.2.27)](https://github.com/kaizhu256/node-jslint-lite) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-jslint-lite.svg)](https://travis-ci.org/npmdoc/node-npmdoc-jslint-lite)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "jslint-lite": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-npmdoc-jslint-lite",
    "keywords": [
        "documentation",
        "jslint-lite"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_jslint_lite.js",
    "name": "npmdoc-jslint-lite",
    "nameAlias": "npmdoc_jslint_lite",
    "nameOriginal": "npmdoc-jslint-lite",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-jslint-lite.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.3"
}
